#include <iostream>
using namespace std;

int main() {
   
   int x, y, z, small;
   cin >> x >> y >> z;
   
   if ((x <= y) && (x <= z)) {
      small = x;
   }
   else if ((y <= x) && (y <= z)) {
      small = y;
   }
   else if ((z <= x) && (z <= y)) {
      small = z;
   }
   
   cout << small << endl;

   return 0;
}