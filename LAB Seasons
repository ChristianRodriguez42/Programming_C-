/* Write a program that takes a date as input and outputs the date's season. 
The input is a string to represent the month and an int to represent the day. 
The dates for each season are:
Spring: March 20 - June 20
Summer: June 21 - September 21
Autumn: September 22 - December 20
Winter: December 21 - March 19   */

#include <iostream>
#include <string>
using namespace std;

int main() {
   string inputMonth;
   int inputDay;
   
   cin >> inputMonth;
   cin >> inputDay;
   
   if (inputMonth == "December") {
      if (inputDay >= 1 && inputDay <= 20) {
         cout << "Autumn" << endl;
      }
      else if (inputDay >= 21 && inputDay <= 31) {
         cout << "Winter" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "January") {
      if (inputDay >= 1 && inputDay <= 31) {
         cout << "Winter" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "February") {
      if (inputDay >= 1 && inputDay <= 29) {
         cout << "Winter" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "March") {
      if (inputDay >= 1 && inputDay <= 19) {
         cout << "Winter" << endl;
      }
      else if (inputDay >= 20 && inputDay <= 31) {
         cout << "Spring" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "April") {
      if (inputDay >= 1 && inputDay <= 30) {
         cout << "Spring" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "May") {
      if (inputDay >= 1 && inputDay <= 31) {
         cout << "Spring" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "June") {
      if (inputDay >= 1 && inputDay <= 20) {
         cout << "Spring" << endl;
      }
      else if (inputDay >= 21 && inputDay <= 30) {
         cout << "Summer" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "July") {
      if (inputDay >= 1 && inputDay <= 31) {
         cout << "Summer" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "August") {
      if (inputDay >= 1 && inputDay <= 31) {
         cout << "Summer" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "September") {
      if (inputDay >= 1 && inputDay <= 21) {
         cout << "Summer" << endl;
      }
      else if (inputDay >= 22 && inputDay <= 30) {
         cout << "Autumn" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "October") {
      if (inputDay >= 1 && inputDay <= 31) {
         cout << "Autumn" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else if (inputMonth == "November") {
      if (inputDay >= 1 && inputDay <= 30) {
         cout << "Autumn" << endl;
      }
      else {
         cout << "Invalid" << endl;
      }
   }
   else {
      cout << "Invalid" << endl;
   }
      

   return 0;
}